{
    "name": "agungsugiarto/codeigniter4-dusk",
    "description": "CodeIgniter4 dusk is fork from Laravel Dusk provides simple end-to-end testing and browser automation.",
    "keywords": ["codeigniter4", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Agung Sugiarto",
            "email": "me.agungsugiarto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "agungsugiarto/codeigniter4-authentication": "^2.0",
        "codeigniter4/framework": "^4.2",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "nesbot/carbon": "^2.0",
        "php-webdriver/webdriver": "^1.9.0",
        "symfony/finder": "^4.3|^5.0|^6.0",
        "symfony/process": "^4.3|^5.0|^6.0",
        "vlucas/phpdotenv": "^3.0|^4.0|^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0"
    },
    "suggest": {
        "ext-pcntl": "Used to gracefully terminate Dusk when tests are running."
    },
    "autoload": {
        "psr-4": {
            "Fluent\\Dusk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluent\\Dusk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
